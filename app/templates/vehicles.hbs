<div class="cars">
  <div class="container-fluid other-pages-background">
    <div class="row">
      <div class="col-md text-center mt-5">
        <h2>Cars Sale In Albuquerque</h2>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="car-items">
      <ul class="results">
        {{#each @model as |vehicle|}}
          <li>
            <Vehicle @vehicle={{vehicle}} />
          </li>
          <div class="row my-2 a">
            <div class="col-md-1 mx-2 text-right">
              <button {{action 'updateCarItem' vehicle}} type="button" class="btn btn-info">Update</button>
            </div>
            <div class="col-md-1 mx-2 text-right">
              <button {{action 'deleteCarItem' vehicle}} type="button" class="btn btn-danger">Delete</button>
            </div>
          </div>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
